<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>view.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>product</servlet-name>
        <servlet-class>view.product</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>products</servlet-name>
        <servlet-class>view.products</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Branches</servlet-name>
        <servlet-class>view.Branches</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Branch</servlet-name>
        <servlet-class>view.Branch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServiceDesk</servlet-name>
        <servlet-class>view.ServiceDesk</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BankingOfficer</servlet-name>
        <servlet-class>view.BankingOfficer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login.view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>product</servlet-name>
        <url-pattern>/product.view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>products</servlet-name>
        <url-pattern>/products.view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Branches</servlet-name>
        <url-pattern>/Branches.view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Branch</servlet-name>
        <url-pattern>/Branch.view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServiceDesk</servlet-name>
        <url-pattern>/ServiceDesk.view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BankingOfficer</servlet-name>
        <url-pattern>/BankingOfficer.view</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>home.html</welcome-file>
    </welcome-file-list>
    </web-app>
